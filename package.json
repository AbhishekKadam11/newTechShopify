{
  "name": "newtechshopify",
  "version": "1.0.0",
  "description": "Ecommerce website based on shopify",
  "main": "index.js",
  "scripts": {
    "dev": "run-p -sr \"shopify:dev -- {@}\" \"vite:dev\" --",
    "deploy": "run-s \"vite:build\" \"shopify:push -- {@}\" --",
    "shopify:dev": "shopify theme dev --environment development",
    "shopify:push": "shopify theme push",
    "vite:dev": "vite",
    "vite:build": "vite build"
  },
  "author": "Abhishek.k",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.42",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.2",
    "vite-plugin-shopify": "^3.0.1"
  },
  "type": "module"
}
